{
  "name": "pay-dev-env",
  "dockerComposeFile": "./docker-compose.yml",
  "service": "dev-container",
  "workspaceFolder": "/workspaces",
  "customizations": {
    "intellij": {
      "plugins": [
        "com.intellij.java",
        "org.jetbrains.plugins.gradle"
      ]
    }
  },
  "postCreateCommand": "bash .devcontainer/postCreateCommand.sh",
  "forwardPorts": [80, 8081, 8082, 9092],
  "remoteUser": "root"
}
